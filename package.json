{
  "name": "clashofclans.js.org",
  "description": "Website with the clashofclans.js docs and guide.",
  "version": "1.0.1",
  "private": true,
  "workspaces": [
    "lib"
  ],
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "git-submodule": "git pull --recurse-submodules && git submodule update --remote --recursive --init"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.9",
    "@docusaurus/plugin-client-redirects": "^2.0.0-beta.9",
    "@docusaurus/preset-classic": "^2.0.0-beta.9",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "docusaurus-plugin-typedoc": "^0.16.4",
    "typedoc": "^0.22.9",
    "typedoc-plugin-markdown": "^3.11.7"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 4,
    "printWidth": 140,
    "singleQuote": true,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "trailingComma": "none",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "useTabs": false,
          "tabWidth": 4
        }
      },
      {
        "files": "*.yml",
        "options": {
          "useTabs": false,
          "tabWidth": 2
        }
      }
    ]
  }
}
